<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Backdrop">
        <attr name="visible_back_height"
            format="dimension"/>

        <attr name="visible_back_view"
            format="reference"/>

        <attr name="min_front_height"
            format="dimension"/>
    </declare-styleable>

    <declare-styleable name="Backdrop_Layout">
        <attr name="layout_layer"
            format="enum">
            <enum name="back" value="0"/>
            <enum name="front" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FrontLayer">
        <attr name="subheader"
            format="string"/>
    </declare-styleable>
</resources>